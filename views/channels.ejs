<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div>
        <h1>Channels</h1>
        <form action="/api/user/login" method="GET">
            <button type="submit">Logout</button>
        </form> 
        <ul>
            <% for(var i=0; i < channels.length; i++) { %>
                <li>
                    <a href="<%= `/api/channels/${channels[i]._id}` %>"><%= channels[i].name %></a>
                </li>
            <% } %>
        </ul>
    </div>

    <div>
        <h1>Your Channels</h1>
        <ul>
            <% for(var i=0; i < adminChannels.length; i++) { %>
                <li>
                    <a href="../channel"><%= adminChannels[i] %></a>
                </li>
            <% } %>
        </ul>
    </div>
    
</body>
</html>